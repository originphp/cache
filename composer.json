{
    "name": "originphp/cache",
    "description": "OriginPHP Cache",
    "type": "library",
    "keywords": [
        "originphp",
        "cache",
        "redis",
        "memcached",
        "file",
        "apcu"
    ],
    "homepage": "https://www.originphp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Jamiel Sharief",
            "email": "js@originphp.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Origin\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Origin\\Test\\Cache\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3.0",
        "originphp/configurable": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "phpstan/phpstan": "^0.12.64"
    },
    "suggest": {
        "ext-apcu": "APCu",
        "ext-memcached": "Memcached",
        "ext-redis": "Redis"
    }
}
